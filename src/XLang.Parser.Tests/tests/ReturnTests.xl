//Hello World Example
public class Program
{
	public static void Main(Array args)
	{
		MSG.Show("Next: Return Test");
		ReturnTest();

		MSG.Show("Next: Clamp Test");
		MSG.Show(Clamp(11, 0, 1).ToString());
		MSG.Show(Clamp(1, 2, 3).ToString());
		MSG.Show(Clamp(1, 0, 2).ToString());

		MSG.Show("Next: Sum Test");
		MSG.Show(Sum(50).ToString());

		MSG.Show("Next: Sum Except Test");
		MSG.Show(SumExcept(50, 1).ToString());

		MSG.Show("Next: Sum Until Test");
		MSG.Show(SumUntil(50).ToString());

		MSG.Show("Next: Prime Test");
		MSG.Show(IsPrime(7).ToString());
		MSG.Show(IsPrime(20).ToString());
		MSG.Show(IsPrime(2).ToString());
		MSG.Show(IsPrime(15632887).ToString());

	}

	private static number IsPrime(number num)
	{
    	if (num <= 1) return 0;
    	if (num == 2) return 1;
    	for (number i = 3; i <= num / 2; i = i + 2)
        	if (num % i == 0)
            	return 0;

    	return 1;        
	}

	private static number SumUntil(number max)
	{
		number ret = 1;
		while( 1 == 1 )
		{
			if(ret > max)
			{
				break;
			}
			ret = ret + ret;
		}

		return ret;
	}

	private static number SumExcept(number max, number except)
	{
		number ret = 0;
		for(number i = 0; i < max; i = i + 1)
		{
			if(i == except)
				continue;
			ret = ret + 1;
		}

		return ret;
	}

	private static number Sum(number max)
	{
		number ret = 0;
		for(number i = 0; i < max; i = i + 1)
		{
			ret = ret + 1;
		}

		return ret;
	}

	private static number Clamp(number num, number min, number max)
	{
		if(num > max)
		{
			return max;
		}
		else if(num < min)
		{
			return min;
		}
		else
		{
			return num;
		}
	}

	private static void ReturnTest()
	{
		MSG.Show("Return Test");
		return;
		MSG.Show("FUCK");
	}
}